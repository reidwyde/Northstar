{
  "name": "Northstar",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "dynamo:view": "npx ts-node --project tsconfig.scripts.json scripts/dynamo-viewer.ts",
    "dynamo:view:all": "npx ts-node --project tsconfig.scripts.json scripts/dynamo-viewer.ts all",
    "dynamo:view:quests": "npx ts-node --project tsconfig.scripts.json scripts/dynamo-viewer.ts type quest",
    "dynamo:view:waypoints": "npx ts-node --project tsconfig.scripts.json scripts/dynamo-viewer.ts type waypoint",
    "dynamo:manage": "npx ts-node --project tsconfig.scripts.json scripts/dynamo-manager.ts",
    "dynamo:clear": "npx ts-node --project tsconfig.scripts.json scripts/dynamo-manager.ts clear",
    "dynamo:migrate": "npx ts-node --project tsconfig.scripts.json scripts/dynamo-manager.ts migrate",
    "dynamo:test": "npx ts-node --project tsconfig.scripts.json scripts/test-dynamodb.ts"
  },
  "jest": {
    "preset": "jest-expo"
  },
